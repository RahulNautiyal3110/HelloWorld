pipeline {
    agent any
    tools {
          maven 'Maven 3.0'
    }
    stages {
        stage('git checkout') {
            steps {
                git credentialsId: 'github', url: 'https://github.com/RahulNautiyal3110/HelloWorld'
            }
        }
         stage('maven') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}
